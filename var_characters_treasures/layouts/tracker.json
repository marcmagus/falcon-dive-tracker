{
    "tracker_horizontal": {
        "type": "container",
        "background": "#000000",
        "content": {
            "type": "array",
            "orientation": "horizontal",
            "dropshadow": true,
            "content": [
                {
                    "type": "array",
                    "orientation": "vertical",
                    "margin": "0,0",
                    "content": [
                        {
                            "type": "group",
                            "header": "Bosses",
                            "content": {
                                "type": "layout",
                                "key": "shared_boss_grid"
                            }
                        },
                        {
                            "type": "group",
                            "header": "Characters",
                            "content": {
                                "type": "itemgrid",
                                "item_margin": "4,4",
                                "rows": [
                                    [
                                        "character-1",
                                        "character-2",
                                        "character-3",
                                        "character-4",
                                        "character-5",
                                    ]
                                ]
                            }
                        },
                        {
                            "type": "group",
                            "header": "Items",
                            "margin": "0,0,3,0",
                            "content": {
                                "type": "layout",
                                "h_alignment": "center",
                                "v_alignment": "center",
                                "key": "shared_item_grid"
                            }
                        },
                    ]
                },
                {
                    "type": "tabbed",
                    "tabs": [
                        {
                            "title": "Overworld",
                            "content": {
                                "type": "map",
                                "maps": [
                                    "overworld"
                                ]
                            }
                        },
                        {
                            "title": "Underground",
                            "content": {
                                "type": "map",
                                "maps": [
                                    "underground"
                                ]
                            }
                        },
                        {
                            "title": "Moon",
                            "content": {
                                "type": "map",
                                "maps": [
                                    "moon"
                                ]
                            }
                        },
                        {
                            "title": "All",
                            "content": {
                                "type": "map",
                                "maps": [
                                    "overworld",
                                    "underground",
                                    "moon"
                                ]
                            }
                        }
                    ]
                }
            ]
        }
    },
    "tracker_default": {
        "type": "dock",
        "background": "#000000",
        "content": [
            {
                "type": "dock",
                "dock": "bottom",
                "content": [
                    {
                        "type": "array",
                        "orientation": "vertical",
                        "dock": "left",
                        "content": [
                            {
                                "type": "group",
                                "header": "Items",
                                "margin": "0,0,3,0",
                                "content": {
                                    "type": "layout",
                                    "h_alignment": "center",
                                    "v_alignment": "center",
                                    "key": "shared_item_grid"
                                }
                            },
                            {
                                "type": "group",
                                "header": "Characters",
                                "margin": "0,0,3,0",
                                "h_alignment": "center",
                                "v_alignment": "center",
                                "content": {
                                    "type": "itemgrid",
                                    "item_margin": "4,4",
                                    "rows": [
                                        [
                                            "character-1",
                                            "character-2",
                                            "character-3",
                                            "character-4",
                                            "character-5",
                                        ]
                                    ]
                                }
                            },
                        ]
                    },
                    {
                        "type": "group",
                        "header": "Bosses",
                        "dock": "right",
                        "content": {
                            "type": "layout",
                            "key": "shared_boss_grid"
                        }
                    }
                ]
            },
            {
                "type": "dock",
                "dock": "top",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "content": [
                    {
                        "type": "map",
                        "h_alignment": "stretch",
                        "v_alignment": "stretch",
                        "maps": [
                            "overworld"
                        ]
                    },
                    {
                        "type": "dock",
                        "dock": "right",
                        "h_alignment": "stretch",
                        "v_alignment": "stretch",
                        "content": [
                            {
                                "type": "map",
                                "maps": [
                                    "underground",
                                ]
                            },
                            {
                                "type": "map",
                                "maps": [
                                    "moon",
                                ]
                            }
                        ]
                    }
                ]
            }
        ]
    }
}
